<!-- PLURA_CONFIG_VERSION: d-sysmon-plura-v3.1-merge-gtfobins-curated-ubuntu.xml -->
<!-- PLURA_SCHEMA_TARGET: 4.90 -->
<!-- DISTRO_TARGET: Ubuntu -->
<!-- PROFILE_TARGET: DESKTOP -->
<Sysmon schemaversion="4.90">
  <HashAlgorithms>*</HashAlgorithms>
  <EventFiltering>
    <!-- =========================================================-->
    <!--  PLURA baseline filters (from sysmon-plura.xml)-->
    <!-- =========================================================-->
    <RuleGroup name="Defult_ProcessCreate" groupRelation="or">
      <ProcessCreate onmatch="exclude">
        <Rule name="PLURA_Syslog" groupRelation="and">
          <Image condition="contains">logger</Image>
          <ParentImage condition="contains">bash</ParentImage>
          <CommandLine condition="contains">-p local0.notice</CommandLine>
          <CommandLine condition="contains">-t bash -i --</CommandLine>
        </Rule>
        <Rule name="PLURA_Execution_Disk Use" groupRelation="and">
          <CurrentDirectory condition="is">/etc/plura</CurrentDirectory>
          <CommandLine condition="begin with">sh -c df -h</CommandLine>
          <CommandLine condition="contains">awk '$NF==</CommandLine>
          <CommandLine condition="end with">$(NF-1)}'</CommandLine>
        </Rule>
        <Rule name="PLURA_Execution_Disk Use_gawk" groupRelation="and">
          <Image condition="is">/usr/bin/gawk</Image>
          <CurrentDirectory condition="is">/etc/plura</CurrentDirectory>
          <CommandLine condition="begin with">awk $NF==</CommandLine>
          <CommandLine condition="contains">{printf</CommandLine>
          <CommandLine condition="end with">, $(NF-1)}</CommandLine>
        </Rule>
        <Rule name="PLURA_Execution_Disk Use_df" groupRelation="and">
          <Image condition="is">/usr/bin/df</Image>
          <CurrentDirectory condition="is">/etc/plura</CurrentDirectory>
          <CommandLine condition="is">df -h</CommandLine>
        </Rule>
        <Rule name="PLURA_Execution_Mem Use_free" groupRelation="and">
          <Image condition="is">/usr/bin/free</Image>
          <CurrentDirectory condition="is">/etc/plura</CurrentDirectory>
          <CommandLine condition="is">free -m</CommandLine>
        </Rule>
        <Rule name="PLURA_Execution_Mem Use" groupRelation="and">
          <CurrentDirectory condition="is">/etc/plura</CurrentDirectory>
          <CommandLine condition="begin with">sh -c free -m | awk 'NR==2{printf "%.2f%%", $3*100/$2 }'</CommandLine>
        </Rule>
        <Rule name="PLURA_Execution_Mem Use_gawk" groupRelation="and">
          <Image condition="is">/usr/bin/gawk</Image>
          <CurrentDirectory condition="is">/etc/plura</CurrentDirectory>
          <CommandLine condition="begin with">awk NR==2{printf</CommandLine>
          <CommandLine condition="end with">, $3*100/$2 }</CommandLine>
        </Rule>
        <Rule name="PLURA_Execution_log gzip" groupRelation="and">
          <Image condition="is">/usr/bin/gzip</Image>
          <CommandLine condition="is">/bin/gzip /var/log/plura/agent/event.log</CommandLine>
          <ParentImage condition="is">/usr/bin/bash</ParentImage>
          <User condition="is">root</User>
        </Rule>
        <Rule name="PLURA_Execution_log rm_weblog" groupRelation="and">
          <Image condition="is">/usr/bin/rm</Image>
          <CommandLine condition="begin with">rm -f /var/log/plura/weblog</CommandLine>
          <ParentImage condition="is">/usr/bin/bash</ParentImage>
          <User condition="is">root</User>
        </Rule>
        <Rule name="PLURA_Execution_log rm_evnetlog" groupRelation="and">
          <Image condition="is">/usr/bin/rm</Image>
          <CommandLine condition="begin with">rm -f /var/log/plura/agent/event.log.gz</CommandLine>
          <ParentImage condition="is">/usr/bin/bash</ParentImage>
          <User condition="is">root</User>
        </Rule>
        <Rule name="PLURA_Execution_log wc" groupRelation="and">
          <Image condition="is">/usr/bin/wc</Image>
          <CommandLine condition="is">wc -c /var/log/plura/agent/event.log</CommandLine>
          <User condition="is">root</User>
        </Rule>
        <Rule name="PLURA_Execution_log mv" groupRelation="or">
          <CommandLine condition="begin with">mv /var/log/plura/weblog*.log /var/log/plura/weblog*.log-</CommandLine>
          <CommandLine condition="begin with">mv /var/log/plura/ceelog-127.0.0.1.log /var/log/plura/ceelog-127.0.0.1.log-</CommandLine>
        </Rule>
        <Rule name="PLURA_Execution_timer" groupRelation="and">
          <Image condition="is">/usr/bin/bash</Image>
          <CommandLine condition="begin with">/bin/bash /etc/plura/plura.sh timer</CommandLine>
        </Rule>
        <Rule name="PLURA_Defend" groupRelation="and">
          <CurrentDirectory condition="is">/etc/plura</CurrentDirectory>
          <CommandLine condition="begin with">iptables -I INPUT -s</CommandLine>
          <CommandLine condition="end with">-j LOG --log-prefix [PLURA Defend]</CommandLine>
        </Rule>
      </ProcessCreate>
    </RuleGroup>
    <!-- =========================================================-->
    <!--  Event ID 3: NetworkConnect (NOISE MIN)                    -->
    <!--  Strategy: Log ONLY high-signal cases.                     -->
    <!-- =========================================================-->
    <RuleGroup name="NoiseMin_NetworkConnect" groupRelation="or">
      <NetworkConnect onmatch="include">
        <!-- Executables running from user-writable / staging locations -->
        <Image condition="begin with">/tmp/</Image>
        <Image condition="begin with">/var/tmp/</Image>
        <Image condition="begin with">/dev/shm/</Image>
        <Image condition="begin with">/run/user/</Image>
        <Image condition="begin with">/var/www/</Image>
        <Image condition="begin with">/srv/www/</Image>
        <!-- Classic reverse-shell / tunneling utilities (rare in normal ops) -->
        <Image condition="end with">/nc</Image>
        <Image condition="end with">/ncat</Image>
        <Image condition="end with">/socat</Image>
        <Image condition="end with">/telnet</Image>
        <Image condition="end with">/ftp</Image>
        <Image condition="end with">/tftp</Image>
        <Image condition="begin with">/home/</Image>
        <Image condition="begin with">/run/media/</Image>
        <Image condition="begin with">/media/</Image>
        <Image condition="begin with">/mnt/</Image>
        <Image condition="begin with">/opt/</Image>
        <Image condition="begin with">/usr/local/bin/</Image>
        <Image condition="begin with">/usr/local/sbin/</Image>
        <Image condition="end with">/curl</Image>
        <Image condition="end with">/wget</Image>
        <Image condition="end with">/openssl</Image>
        <Image condition="end with">/ssh</Image>
        <Image condition="end with">/scp</Image>
        <Image condition="end with">/sftp</Image>
        <Image condition="contains any">/python;/perl;/ruby;/php;/node</Image>
        <Image condition="end with">/bash</Image>
        <Image condition="end with">/sh</Image>
      </NetworkConnect>
    </RuleGroup>
    <!-- Defense-in-depth: keep PLURA agent traffic out even if it matches future includes -->
    <RuleGroup name="Exclude_PLURA_NetworkConnect" groupRelation="or">
      <NetworkConnect onmatch="exclude">
        <Image condition="contains">/usr/local/sbin/plurad</Image>
        <Image condition="is">plurad</Image>
      </NetworkConnect>
    </RuleGroup>
    <!-- =========================================================-->
    <!--  Event ID 5: ProcessTerminate (NOISE MIN)                  -->
    <!--  Strategy: Only log termination of suspicious processes.   -->
    <!-- =========================================================-->
    <RuleGroup name="NoiseMin_ProcessTerminate" groupRelation="or">
      <ProcessTerminate onmatch="include">
        <!-- Processes executed from typical attacker staging dirs -->
        <Image condition="begin with">/tmp/</Image>
        <Image condition="begin with">/var/tmp/</Image>
        <Image condition="begin with">/dev/shm/</Image>
        <Image condition="begin with">/run/user/</Image>
        <Image condition="begin with">/var/www/</Image>
        <Image condition="begin with">/srv/www/</Image>
        <!-- Reverse shell / tunneling utilities -->
        <Image condition="end with">/nc</Image>
        <Image condition="end with">/ncat</Image>
        <Image condition="end with">/socat</Image>
        <Image condition="begin with">/home/</Image>
        <Image condition="begin with">/run/media/</Image>
        <Image condition="begin with">/media/</Image>
        <Image condition="begin with">/mnt/</Image>
        <Image condition="begin with">/opt/</Image>
        <Image condition="begin with">/usr/local/bin/</Image>
        <Image condition="begin with">/usr/local/sbin/</Image>
      </ProcessTerminate>
    </RuleGroup>
    <RuleGroup name="Defult_RawAccessRead" groupRelation="or">
      <RawAccessRead onmatch="exclude" />
    </RuleGroup>
    <RuleGroup name="Defult_FileCreate" groupRelation="or">
      <FileCreate onmatch="exclude">
        <Rule name="PLURA_Execution_event.log" groupRelation="and">
          <Image condition="is">/usr/local/sbin/plurad</Image>
          <TargetFilename condition="is">/var/log/plura/agent/event.log</TargetFilename>
        </Rule>
        <Rule name="PLURA_Execution_event.log" groupRelation="and">
          <Image condition="is">/usr/local/sbin/plurad</Image>
          <TargetFilename condition="is">/etc/plura/temp/.sysfilter-ref.json</TargetFilename>
        </Rule>
        <Rule name="PLURA_Execution_plurad.job" groupRelation="and">
          <Image condition="is">/usr/local/sbin/plurad</Image>
          <TargetFilename condition="is">/run/plurad.job</TargetFilename>
        </Rule>
        <Rule name="PLURA_Execution_event.log.gz" groupRelation="and">
          <Image condition="is">/usr/bin/gzip</Image>
          <TargetFilename condition="is">/var/log/plura/agent/event.log.gz</TargetFilename>
        </Rule>
        <Rule name="RSYSLOG_Execution_log" groupRelation="and">
          <Image condition="is">/usr/sbin/rsyslogd</Image>
          <TargetFilename condition="is">/var/log/plura/ceelog-127.0.0.1.log</TargetFilename>
          <User condition="is">syslog</User>
        </Rule>
        <Rule name="RSYSLOG_tmp_log" groupRelation="and">
          <Image condition="is">/usr/sbin/rsyslogd</Image>
          <TargetFilename condition="is">/etc/plura/temp/.sysfilter-ref.json</TargetFilename>
        </Rule>
        <Rule name="RSYSLOG_tmp_log" groupRelation="and">
          <Image condition="is">/usr/sbin/rsyslogd</Image>
          <TargetFilename condition="is">/var/lib/rsyslog/imjournal.state.tmp</TargetFilename>
        </Rule>
      </FileCreate>
    </RuleGroup>
    <!-- =========================================================-->
    <!--  Event ID 23: FileDelete (NOISE MIN)                       -->
    <!--  Strategy: Log ONLY high-signal deletions.                 -->
    <!-- =========================================================-->
    <RuleGroup name="NoiseMin_FileDelete" groupRelation="or">
      <FileDelete onmatch="include">
        <Rule groupRelation="and">
          <TargetFilename condition="begin with">/home/</TargetFilename>
          <TargetFilename condition="contains any">/.config/autostart/;/.config/systemd/user/;/.local/share/applications/;/.local/bin/</TargetFilename>
        </Rule>
        <Rule groupRelation="and">
          <TargetFilename condition="begin with">/home/</TargetFilename>
          <TargetFilename condition="contains any">/Documents/;/Desktop/;/Downloads/;/Pictures/;/Videos/;/Music/</TargetFilename>
          <TargetFilename condition="contains any">.doc;.docx;.xls;.xlsx;.ppt;.pptx;.pdf;.hwp;.odt;.ods;.odp;.csv;.txt;.rtf</TargetFilename>
        </Rule>
        <!-- Deleting scripts/binaries from common staging locations -->
        <Rule groupRelation="and">
          <TargetFilename condition="contains any">/tmp/;/var/tmp/;/dev/shm/;/run/user/;/var/www/;/srv/www/;/run/media/;/media/;/mnt/</TargetFilename>
          <TargetFilename condition="contains any">.sh;.bash;.zsh;.ksh;.py;.pl;.rb;.jar;.so;.bin;.run;.elf;.out;.AppImage</TargetFilename>
        </Rule>
        <!-- History / evidence removal -->
        <TargetFilename condition="end with">/.bash_history</TargetFilename>
        <TargetFilename condition="end with">/.zsh_history</TargetFilename>
        <TargetFilename condition="end with">/.ash_history</TargetFilename>
        <TargetFilename condition="end with">/.sh_history</TargetFilename>
        <TargetFilename condition="end with">/.mysql_history</TargetFilename>
        <TargetFilename condition="end with">/.psql_history</TargetFilename>
        <!-- SSH persistence / credential material -->
        <TargetFilename condition="end with">/.ssh/authorized_keys</TargetFilename>
        <TargetFilename condition="end with">/.ssh/id_rsa</TargetFilename>
        <TargetFilename condition="end with">/.ssh/id_ed25519</TargetFilename>
        <TargetFilename condition="end with">/.ssh/known_hosts</TargetFilename>
        <!-- Rare but high-impact system files -->
        <TargetFilename condition="is">/etc/passwd</TargetFilename>
        <TargetFilename condition="is">/etc/shadow</TargetFilename>
        <TargetFilename condition="is">/etc/sudoers</TargetFilename>
        <TargetFilename condition="is">/etc/ssh/sshd_config</TargetFilename>
        <TargetFilename condition="contains">/.mozilla/firefox/</TargetFilename>
        <TargetFilename condition="end with">logins.json</TargetFilename>
        <TargetFilename condition="end with">key4.db</TargetFilename>
        <TargetFilename condition="end with">cookies.sqlite</TargetFilename>
        <TargetFilename condition="contains">/.config/google-chrome/</TargetFilename>
        <TargetFilename condition="contains">/.config/chromium/</TargetFilename>
        <TargetFilename condition="end with">Login Data</TargetFilename>
        <TargetFilename condition="end with">Cookies</TargetFilename>
        <TargetFilename condition="contains">/.local/share/keyrings/</TargetFilename>
        <TargetFilename condition="contains">/.kde/share/apps/kwallet/</TargetFilename>
      </FileDelete>
    </RuleGroup>
    <!-- OS-specific: suppress package-manager/cache churn even if it matches the above -->
    <RuleGroup name="Exclude_PackageManager_FileDelete_Ubuntu" groupRelation="or">
      <FileDelete onmatch="exclude">
        <Rule groupRelation="and">
          <Image condition="contains any">/apt;/apt-get;/aptitude;/dpkg;/dpkg-deb;/dpkg-query;/dpkg-preconfigure;/unattended-upgrade;/apt.systemd.daily;/snap;/snapd;/snapctl;/flatpak;/packagekitd;/pkcon</Image>
          <TargetFilename condition="contains any">/tmp/;/var/tmp/;/dev/shm/;/run/user/;/var/cache/apt/;/var/lib/apt/;/var/lib/dpkg/;/var/cache/debconf/;/var/log/apt/;/var/cache/snapd/;/var/lib/snapd/;/var/snap/;/var/cache/PackageKit/;/var/lib/PackageKit/</TargetFilename>
        </Rule>
      </FileDelete>
    </RuleGroup>
    <!-- =========================================================-->
    <!--  GTFOBins additions (from s-sysmon-plura-v3.1-gtfobins-curated.xml )-->
    <!--  NOTE: If an event tag uses onmatch="include", Sysmon will log ONLY matched events for that tag.-->
    <!-- =========================================================-->
    <RuleGroup name="Include_MalwareDrop_Linux" groupRelation="or">
      <FileCreate onmatch="include">
        <Rule groupRelation="and">
          <TargetFilename condition="begin with">/home/</TargetFilename>
          <TargetFilename condition="contains any">/Documents/;/Desktop/;/Downloads/;/Pictures/;/Videos/;/Music/;/Client_Contracts/</TargetFilename>
          <TargetFilename condition="contains any">.sh;.bash;.zsh;.ksh;.py;.pl;.rb;.php;.js;.jar;.so;.bin;.run;.elf;.out;.AppImage</TargetFilename>
        </Rule>
        <Rule groupRelation="and">
          <TargetFilename condition="begin with">/root/</TargetFilename>
          <TargetFilename condition="contains any">/Downloads/;/Documents/;/Desktop/</TargetFilename>
          <TargetFilename condition="contains any">.sh;.bash;.py;.pl;.rb;.php;.js;.jar;.so;.bin;.run;.elf;.out;.AppImage</TargetFilename>
        </Rule>
        <Rule groupRelation="and">
          <TargetFilename condition="begin with">/tmp/</TargetFilename>
          <TargetFilename condition="contains any">.sh;.bash;.py;.pl;.rb;.php;.js;.jar;.so;.bin;.run;.elf;.out;.AppImage</TargetFilename>
        </Rule>
        <Rule groupRelation="and">
          <TargetFilename condition="begin with">/var/tmp/</TargetFilename>
          <TargetFilename condition="contains any">.sh;.bash;.py;.pl;.rb;.php;.js;.jar;.so;.bin;.run;.elf;.out;.AppImage</TargetFilename>
        </Rule>
        <Rule groupRelation="and">
          <TargetFilename condition="begin with">/dev/shm/</TargetFilename>
          <TargetFilename condition="contains any">.sh;.bash;.py;.pl;.rb;.php;.js;.jar;.so;.bin;.run;.elf;.out;.AppImage</TargetFilename>
        </Rule>
        <Rule groupRelation="and">
          <TargetFilename condition="begin with">/run/user/</TargetFilename>
          <TargetFilename condition="contains any">.sh;.bash;.py;.pl;.rb;.php;.js;.jar;.so;.bin;.run;.elf;.out;.AppImage</TargetFilename>
        </Rule>
        <Rule groupRelation="and">
          <TargetFilename condition="contains any">/var/www/;/srv/www/</TargetFilename>
          <TargetFilename condition="contains any">.php;.jsp;.asp;.aspx;.cgi;.pl;.py;.sh;.so;.bin;.elf</TargetFilename>
        </Rule>
        <Rule groupRelation="and">
          <TargetFilename condition="contains any">/run/media/;/media/;/mnt/</TargetFilename>
          <TargetFilename condition="contains any">.sh;.bash;.zsh;.ksh;.py;.pl;.rb;.php;.js;.jar;.so;.bin;.run;.elf;.out;.AppImage</TargetFilename>
        </Rule>
      </FileCreate>
    </RuleGroup>
    <RuleGroup name="Include_GTFOBins_Writers_Linux_Curated" groupRelation="or">
      <FileCreate onmatch="include">
        <Rule groupRelation="and">
          <Image condition="contains any">/curl;/wget;/python;/python3;/perl;/ruby;/php;/node;/ssh;/scp;/sftp;/nc;/ncat;/socat;/ftp;/tftp;/base64;/openssl;/busybox</Image>
          <TargetFilename condition="contains any">/tmp/;/var/tmp/;/dev/shm/;/run/user/;/home/;/root/;/var/www/;/opt/;/srv/;/var/lib/;/run/media/;/media/;/mnt/</TargetFilename>
        </Rule>
        <Rule groupRelation="and">
          <Image condition="contains any">/tar;/unzip;/7z;/gzip;/bzip2;/xz</Image>
          <TargetFilename condition="contains any">/tmp/;/var/tmp/;/dev/shm/;/run/user/;/home/;/root/;/run/media/;/media/;/mnt/</TargetFilename>
        </Rule>
        <Rule groupRelation="and">
          <Image condition="contains any">/bash;/sh;/dash;/zsh;/ksh</Image>
          <TargetFilename condition="contains any">/tmp/;/var/tmp/;/dev/shm/;/run/user/;/run/media/;/media/;/mnt/</TargetFilename>
          <TargetFilename condition="contains any">.sh;.bash;.zsh;.ksh;.py;.pl;.rb;.php;.js;.jar;.so;.bin;.run;.elf;.out;.AppImage</TargetFilename>
        </Rule>
      </FileCreate>
    </RuleGroup>
    <RuleGroup name="Exclude_LegitActivity_Linux" groupRelation="or">
      <FileCreate onmatch="exclude">
        <Rule groupRelation="and">
          <!-- Ubuntu package manager / installer noise -->
          <Image condition="contains any">/apt;/apt-get;/aptitude;/dpkg;/dpkg-deb;/dpkg-query;/dpkg-preconfigure;/unattended-upgrade;/apt.systemd.daily;/snap;/snapd;/snapctl;/flatpak;/packagekitd;/pkcon</Image>
          <TargetFilename condition="contains any">/tmp/;/var/tmp/;/dev/shm/;/run/user/;/var/cache/apt/;/var/lib/apt/;/var/lib/dpkg/;/var/cache/debconf/;/var/log/apt/;/var/cache/snapd/;/var/lib/snapd/;/var/snap/;/var/cache/PackageKit/;/var/lib/PackageKit/</TargetFilename>
        </Rule>
        <TargetFilename condition="end with">.swp</TargetFilename>
        <TargetFilename condition="end with">.swx</TargetFilename>
        <TargetFilename condition="end with">.tmp</TargetFilename>
        <TargetFilename condition="end with">.part</TargetFilename>
        <TargetFilename condition="end with">.crdownload</TargetFilename>
        <TargetFilename condition="end with">~</TargetFilename>
      </FileCreate>
    </RuleGroup>
    <RuleGroup name="Include_Temp_Execution_Linux" groupRelation="or">
      <ProcessCreate onmatch="include">
        <Image condition="begin with">/tmp/</Image>
        <Image condition="begin with">/var/tmp/</Image>
        <Image condition="begin with">/dev/shm/</Image>
        <Image condition="begin with">/run/user/</Image>
        <Image condition="begin with">/home/</Image>
        <Image condition="begin with">/run/media/</Image>
        <Image condition="begin with">/media/</Image>
        <Image condition="begin with">/mnt/</Image>
        <Image condition="begin with">/opt/</Image>
        <Image condition="begin with">/usr/local/bin/</Image>
        <Image condition="begin with">/usr/local/sbin/</Image>
        <Image condition="end with">/nc</Image>
        <Image condition="end with">/ncat</Image>
        <Image condition="end with">/socat</Image>
        <Image condition="end with">/telnet</Image>
        <Image condition="end with">/ftp</Image>
        <Image condition="end with">/tftp</Image>
        <Image condition="end with">/curl</Image>
        <Image condition="end with">/wget</Image>
      </ProcessCreate>
    </RuleGroup>
    <RuleGroup name="Include_Interpreter_Execution_UserPaths_Linux_Desktop" groupRelation="or">
      <ProcessCreate onmatch="include">
        <Rule groupRelation="and">
          <Image condition="contains any">/python;/python3;/perl;/ruby;/php;/node;/java;/bash;/sh;/dash;/zsh;/ksh</Image>
          <CommandLine condition="contains any">/tmp/;/var/tmp/;/dev/shm/;/run/user/;/home/;/run/media/;/media/;/mnt/;/Downloads/;/Desktop/;/Documents/;/.cache/;/.local/bin/;/.config/autostart/;/.config/systemd/user/</CommandLine>
        </Rule>
      </ProcessCreate>
    </RuleGroup>
    <RuleGroup name="Include_Persistence_FileCreate_Linux_Desktop" groupRelation="or">
      <FileCreate onmatch="include">
        <Rule groupRelation="and">
          <TargetFilename condition="begin with">/home/</TargetFilename>
          <TargetFilename condition="contains any">/.config/autostart/;/.config/systemd/user/;/.local/share/applications/;/.local/bin/</TargetFilename>
          <TargetFilename condition="contains any">.desktop;.service;.timer;.socket;.path;.target;.sh;.bash;.zsh;.py;.pl;.rb;.js;.jar;.run;.AppImage</TargetFilename>
        </Rule>
        <Rule groupRelation="and">
          <TargetFilename condition="contains any">/etc/xdg/autostart/;/etc/systemd/system/;/etc/systemd/user/;/usr/local/lib/systemd/system/;/usr/local/share/applications/</TargetFilename>
          <TargetFilename condition="contains any">.desktop;.service;.timer;.socket;.path;.target</TargetFilename>
        </Rule>
        <Rule groupRelation="and">
          <TargetFilename condition="contains any">/etc/cron.d/;/etc/cron.hourly/;/etc/cron.daily/;/etc/cron.weekly/;/etc/cron.monthly/;/var/spool/cron/</TargetFilename>
        </Rule>
      </FileCreate>
    </RuleGroup>
  </EventFiltering>
</Sysmon>
